        <Param val="if (((allResponses.length &gt;= 3) &amp;&amp; (util.sum(allResponses.slice((- 3))) === 0))) {&amp;#10;    trials.finished = true;&amp;#10;    blocks.finished = true;&amp;#10;} else {&amp;#10;    if ((util.sum(allResponses.slice((- 2))) === 2)) {&amp;#10;        trials.finished = true;&amp;#10;        allResponses = [];&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
